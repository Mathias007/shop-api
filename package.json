{
    "name": "shop-api",
    "version": "0.1.0",
    "description": "Serwer backendowy dostarczający REST API odpowiadające na podstawowe zapytania dotyczące produktów w sklepie (CRUD), napisany z wykorzystaniem Node.js we frameworku Express, z połączeniem z nierelacyjną bazą danych MongoDB.",
    "main": "src/index.js",
    "license": "MIT",
    "scripts": {
        "start": "babel-node src/index.js",
        "start:dev": "nodemon --exec babel-node src/index.js",
        "build": "babel src --out-dir dist",
        "serve": "node dist/index.js"
    },
    "dependencies": {
        "@babel/cli": "^7.13.14",
        "@babel/core": "^7.13.14",
        "@babel/node": "^7.13.13",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/plugin-proposal-export-default-from": "^7.12.13",
        "@babel/plugin-transform-async-to-generator": "^7.13.0",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.20.2",
        "@types/express": "^4.17.14",
        "@types/mongoose": "^5.11.97",
        "body-parser": "^1.20.1",
        "core-js": "^3.9.1",
        "cors": "^2.8.5",
        "dotenv": "^9.0.2",
        "express": "4.17.1",
        "mongoose": "^5.12.2"
    }
}
