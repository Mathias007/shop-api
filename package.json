{
    "name": "shop-api",
    "version": "0.1.0",
    "description": "Serwer backendowy dostarczający REST API odpowiadające na podstawowe zapytania dotyczące produktów w sklepie (CRUD), napisany z wykorzystaniem Node.js we frameworku Express, z połączeniem z nierelacyjną bazą danych MongoDB.",
    "main": "src/index",
    "license": "MIT",
    "scripts": {
        "start": "ts-node-esm src/index",
        "nodemon": "nodemon --exec ts-node-esm src/index",
        "serve": "node dist/index"
    },
    "dependencies": {
        "body-parser": "^1.20.1",
        "core-js": "^3.9.1",
        "cors": "^2.8.5",
        "dotenv": "^9.0.2",
        "express": "4.17.1",
        "mongoose": "^5.12.2"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.18.6",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.14",
        "@types/node": "17.0.23",
        "@types/mongoose": "^5.11.97",
        "ts-jest": "^26.5.2",
        "ts-loader": "^8.0.17",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.8.4"
    }
}
